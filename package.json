{
	"name": "obsidian-tag-navigator",
	"version": "1.0.1",
	"description": "An intelligent navigation plugin that provides smart next/previous navigation based on tags and frontmatter properties.",
	"main": "main.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/wesleyel/obsidian-tag-navigator.git"
	},
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:ci": "jest --ci --coverage --watchAll=false"
	},
	"keywords": [
		"obsidian",
		"tags",
		"navigation",
		"frontmatter"
	],
	"author": "wesley",
	"license": "MIT",
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@types/jest": "^29.5.5",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^30.0.0",
		"obsidian": "latest",
		"ts-jest": "^29.1.1",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
